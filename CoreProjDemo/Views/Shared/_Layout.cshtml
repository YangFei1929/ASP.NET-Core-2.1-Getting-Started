<!DOCTYPE html>

<html>
<head>
    @*<meta name="viewport" content="width=device-width" />*@
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no" />
    <title>@ViewBag.Title</title>
    <environment include="Development">
        <link  rel="stylesheet" asp-href-include="css/*" asp-href-exclude="css/ionicons.min.css"/>
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" asp-href-include="css/ionicons.min.css" />
</environment>

    @*<link rel="stylesheet" href="~/css/bootstrap.css" />*@
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">
                    <img asp-append-version="true" alt="Brand" src="~/images/bootstrapDownload.png">
                    @ViewBag.Title
                </a>
            </div>
        </div>
    </nav>
    <div>
        @RenderBody()
    </div>
</body>
</html>
